        <!-- Product -->
        <div class="bg0 m-t-50 p-b-140">
          <div class="container">
              <div class="flex-w flex-sb-m p-b-52">

                  <!-- Search product -->
                  <div class="dis-none panel-search w-full p-t-10 p-b-15">
                      <div class="bor8 dis-flex p-l-15">
                          <button class="size-113 flex-c-m fs-16 cl2 hov-cl1 trans-04">
                              <i class="zmdi zmdi-search"></i>
                          </button>

                          <input class="mtext-107 cl2 size-114 plh2 p-r-15" type="text" name="search-product"
                              placeholder="Search">
                      </div>
                  </div>

              </div>

              <!-- ============ Main ============ -->

              <section class="page-header">

              </section>

              <div class="page-wrapper">
                  <div class="checkout shopping">
                      <div class="container">
                          <div class="row">
                              <form class="checkout-form" action="/checkout/<%=address._id %>" method="post" id="checkout-form">
                                  <div class="col-md-8">
                                      <div class="block billing-details">
                                          <h4 class="widget-title" style="color: #717fe0;">Billing Details</h4>                                            
                                          <div class="form-group p-b-5 same-line">
                                              <div class="form-group same-line-in">
                                                  <label for="full_name"></label>
                                                  <input type="text" name="name" class="form-control" id="full_name"
                                                      value="<%= address.name %>" placeholder="Full Name:"
                                                      style="width:370px; border: 1px solid #222;">
                                              </div>
                                              <div class="form-group p-l-10 same-line-in">
                                                  <label for="user_address"></label>
                                                  <input type="tel" name="phone" class="form-control"
                                                      id="user_address" value="<%= address.phone %>"
                                                      placeholder="Phone Number:"
                                                      style="width:370px; border: 1px solid #222;">
                                              </div>
                                          </div>

                                          <div class="form-group">
                                              <label for="user_address"></label>
                                              <input type="text" name="address" class="form-control" id="user_address"
                                                  value="<%= address.address %>" placeholder="Address:"
                                                  style="height: 50px; border: 1px solid #222;">
                                          </div>
                                          <div class="form-group p-b-5 same-line">
                                              <div class="form-group same-line-in">
                                                  <label for="user_city"></label>
                                                  <input type="text" name="city" class="form-control" id="user_city"
                                                      name="city" placeholder="City: " value="<%= address.city %>"
                                                      style="width:370px; border: 1px solid #222;">
                                              </div>
                                              <div class="form-group p-l-10 same-line-in">
                                                  <label for="user_country"></label>
                                                  <input type="text" name="state" class="form-control"
                                                      id="user_country" placeholder="State:"
                                                      value="<%= address.state %>"
                                                      style="width:370px; border: 1px solid #222;">
                                              </div>
                                          </div>
                                          <div class="checkout-country-code clearfix">
                                              <div class="form-group">
                                                  <label for="user_post_code"></label>
                                                  <input type="tel" name="pincode" class="form-control"
                                                      id="user_post_code" placeholder="Zip Code: "
                                                      value="<%= address.pincode %>" style="border: 1px solid #222;">
                                              </div>
                                          </div>
                                      </div>
                                      <div class="block">
                                          <h2 class="widget-title" style="color: #717fe0;">Payment Methods</h2>
                                          <div class="payment-methods mt-4">
                                              <div class="payment-method">
                                                  <input class="payment-method-input" type="radio" name="payment"
                                                      id="card-payment" value="paypal" >
                                                  <label class="payment-method-label" for="card-payment">
                                                      <span class="payment-method-icon"><i
                                                              class="fa fa-credit-card"></i></span>
                                                      Paypal
                                                  </label>
                                              </div>
                                              <div class="payment-method">
                                                  <input class="payment-method-input" type="radio" name="payment"
                                                      id="razorpay-payment" value="razorpay">
                                                  <label class="payment-method-label" for="razorpay-payment">
                                                      <span class="payment-method-icon"><i class="fa fa-money"></i></span>
                                                      Razorpay
                                                  </label>
                                              </div>
                                              <div class="payment-method">
                                                  <input class="payment-method-input" type="radio" name="payment"
                                                      id="cod-payment" value="cod">
                                                  <label class="payment-method-label" for="cod-payment">
                                                      <span class="payment-method-icon"><i class="fa fa-money"></i></span>
                                                      Cash on Delivery
                                                  </label>
                                              </div>
                                          </div>
                                          <button type="submit" class="btn mt-20" style="background-color: #717fe0; color: white; border: 2px solid #222;">
                                              Place Order
                                          </button>                                           
                                      </div> 
                                  </div>
                              </form>
                              <div class="col-md-4 border-dark rounded" style="border: 2px solid #222;">
                                  <div class="product-checkout-details">
                                      <div class="block pt-3 pb-3">
                                          <h4 class="widget-title mb-2" style="color: #717fe0;">Order Summary</h4>

                                          <% cart.products.forEach(function(product) { %>
                                              <div class="media product-card ">
                                                  <a class="pull-left mt-4" href="product-single.html">
                                                      <img class="media-object img-thumbnail"
                                                          src="/<%= product.productId.photo[0] %>" alt="image"
                                                          width="70px" />
                                                  </a>
                                                  <div class="media-body p-4">
                                                      <h4 class="media-heading"
                                                          style="font-weight: bold;font-size: 15px;">
                                                          <%= product.productId.name %><a
                                                                  href="product-single.html"></a>
                                                      </h4>
                                                      <p class="price">
                                                          â‚¹&nbsp; <%= product.productId.price %>
                                                      </p>
                                                      <span>Quantity: <%= product.quantity %></span>

                                                      <td class="total price">
                                                          <% var total=product.quantity * product.productId.price; %>
                                                          <p id="product-total-price-<%= product.productId._id %>" class="item-total">
                                                              <%= total %>
                                                          </p>
                                                      </td>
                                                      <hr>
                                                      <!-- <span class="remove">Remove</span> -->
                                                  </div>                                                
                                              </div>                                                                                                   
                                          <% }); %>

                                          <div class="discount-code">
                                              <p style="font-weight: bold;font-size: 15px;">Have a discount ?
                                                  <a data-toggle="modal" data-target="#coupon-modal"
                                                      href="#!">enter it here</a>
                                              </p>
                                              <form action="/redeemCoupon" method="post" id="couponForm">
                                                  <div class="flex-w flex-m m-r-20 m-tb-5">
                                                      <input id="couponCode" class="stext-104 cl2 plh4 size-117 bor13 p-lr-20 m-r-10 m-tb-5" type="text" name="coupon" placeholder="Coupon Code">
                                                      <div id="couponMessage" style="padding-bottom: 10px; color: darkred;"></div>
                                                      <div id="couponMessage1" style="padding-bottom: 10px; color: rgb(7, 126, 13);"></div>
                                                          <button type="submit" class="" onclick="redeem()">
                                                              Apply coupon
                                                          </button>
                                                      
                                                  </div>
                                              </form>
                                          </div>
                                          <ul class="summary-prices">
                                              <li>
                                                  <span style="font-weight: bold;font-size: 15px;">
                                                      Subtotal:
                                                  </span>
                                                  <span class="price" id="subtotal"></span>
                                              </li>
                                              <li>
                                                  <span style="font-weight: bold;font-size: 15px;">
                                                      Shipping:
                                                  </span>
                                                  <span>Free</span>
                                              </li>
                                          </ul>
                                          <div class="summary-total">
                                              <span style="font-weight: bold;font-size: 30px;">
                                                  Total: 
                                              </span>
                                              <span id="totalprice" style="font-weight: bold;font-size: 25px;"></span>
                                          </div>
                                      </div>
                                  </div>
                              </div>
                          </div>
                      </div>
                  </div>
              </div>
          </div>
      </div>



      <main id="main" role="main">
        <section id="checkout-container">
          <div class="container">
            <form class="needs-validation" id="checkoutForm" method="post">
              <div class="row py-5">
                <div class="col-md-4 order-md-2 mb-4" id="couponDiv">
                  <h4 class="d-flex justify-content-between align-items-center mb-3">
                    <span class="text-muted">PRICE DETAILS</span>
                  </h4>
                  <% cart.products.forEach(function(product) { %>
                  <ul class="list-group mb-3">
                    <li class="list-group-item d-flex justify-content-between lh-condensed">
                      <div>
                        <h6 class="my-0">Total Price</h6>
                      </div>
                      <% var total=product.quantity * product.productId.price; %>
                      <span id="product-total-price-<%= product.productId._id %>" class="item-total text-muted">Rs.<%=total%></span>
                    </li>
                    <span id="cartList" hidden>{{cartList}}</span>
                    <li class="list-group-item d-flex justify-content-between lh-condensed">
                      <div>
                        <h6 class="my-0">Delivery</h6>
                        <small class="text-muted">Free Delivery only On Flag-ship Phones</small>
                      </div>
                      <span class="text-muted">Rs.0</span>
                    </li>
                    <li class="list-group-item d-flex justify-content-between bg-light">
                      <div class="text-success">
                        <h6 class="my-0">Coupon</h6>
                        <small id="couponName"></small>
                      </div>
                      <span class="text-success" id="couponPrice">Rs.0</span>
                    </li>
                    <li class="list-group-item d-flex justify-content-between">
                      <span>Amount Payable (INR)</span>
                      <strong id="strong">Rs.<%=total%></strong>
                      <input type="text" id="total" name="total" value="<%=total%>" hidden>
                    </li>
                  </ul>
                  <form class="card p-2">
                    <div class="input-group">
                      <input type="text" class="form-control" id="couponCode" placeholder="Promo code">
                      <div class="input-group-append">
                        <span onclick="submitCoupon()" class="btn btn-secondary">Redeem</span>
                      </div>
                    </div>
                    <span id="invalidCoupon" class="text-danger"></span>
                  </form>
                  <% }); %>
                </div>
                <div class="col-md-8 order-md-1">
                  <h4 class="mb-3">Billing address</h4>
    
                  <div class="row">
                    <div class="col-md-12 mb-3">
                      <div class="col-md-12">
                        
                            <div class="d-flex mb-3">
                              <input type="radio" value="<%= address.id%>" name="address" class=" mr-3">
                              <div class="border p-2" style="color: rgb(80, 80, 80);">
                                <b>
                                  <%= address.name%>
                                </b>, <br>
                                <%= address.address%>, <br>
                                  <%= address.city%>, <%= address.state%>, <br>
                                      Zipcode: <%= address.zipcode%>, Mobile: <%= address.mobile%>
                              </div>
                              <div class="ml-auto pr-3 d-flex">
                                <div>
                                  <a class="btn btn-primary pl-3 pr-3 text-light"
                                    style="letter-spacing: 0.1rem;cursor: pointer;" data-toggle="modal"
                                    data-target="#edit<%= address.id%>">Edit</a>
                                </div>
                                <div class="ml-3 mt-1" style="cursor: pointer;">
                                  <a data-toggle="modal" data-target="#deleteAddr<%= address.id%>">
                                    <div class="border border-warning text-center" style="width: fit-content;">
                                      <span class="ion-ios-close p-3"></span>
                                  </a>
                                </div>
                              </div>
                            </div>
                      </div>
                      
                    </div>
                  </div>
                </div>
    
                <hr class="mb-4">
                <div class="custom-control">
                  <a class="text-primary" data-toggle="modal" data-target=".bd-example-modal-lg"
                    style="cursor: pointer;">Add a new Address</a>
                </div>
                <div class="custom-control custom-checkbox">
                  <input type="checkbox" class="custom-control-input" checked disabled id="same-address">
                  <label class="custom-control-label" for="same-address">Shipping address is the same as my billing
                    address</label>
                </div>
                <hr class="mb-4">
                <h4 class="mb-3">Payment</h4>
                <div class="d-block my-3">
                  <div class="custom-control custom-radio">
                    <input id="COD" name="payment" onclick="validateSubmit()" type="radio"
                      class="custom-control-input" value="COD" required>
                    <label class="custom-control-label" for="COD">Cash on Delivery</label>
                  </div>
                  <div class="custom-control custom-radio">
                    <input id="razorpay" name="payment" onclick="validateSubmit()" type="radio"
                      class="custom-control-input" value="onlineRazorpay" required>
                    <label class="custom-control-label" for="razorpay"><img
                        src="data:image/png;base64,"
                        style="height: 20px;width: 21px;" alt="">Razor Pay</label>
                  </div>
                  <div class="custom-control custom-radio">
                    <input id="paypal" name="payment" onclick="validateSubmit()" type="radio"
                      class="custom-control-input" value="onlinePaypal" required>
                    <label class="custom-control-label" for="paypal"><img
                        src="https://www.paypalobjects.com/paypal-ui/logos/svg/paypal-mark-color.svg"
                        style="height: 20px;width: 21px;" alt="">PayPal</label>
                  </div>
                </div>
                <hr class="mb-4">
    
                <button id="checkoutBtn" class="btn btn-primary btn-lg btn-block" disabled type="submit">Continue to
                  checkout</button>
            </form>
          </div>
          </div>
          </div>
        </section>
      </main>


        